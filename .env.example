# AI Model API Keys
ANTHROPIC_API_KEY=your_anthropic_api_key_here
OPENAI_API_KEY=your_openai_api_key_here

# Optional: Other LLM providers
GOOGLE_API_KEY=your_google_api_key_here
AZURE_OPENAI_API_KEY=your_azure_api_key_here

# Provider selection (anthropic, openai, google, azure)
PROVIDER=anthropic

# Model selection (optional, defaults based on provider)
# For Anthropic: claude-3-5-sonnet-20241022, claude-3-opus-20240229, etc.
# For OpenAI: gpt-4-turbo-preview, gpt-4, gpt-3.5-turbo, etc.
MODEL=claude-3-5-sonnet-20241022

# Configuration
RIGEL_LOG_LEVEL=info
